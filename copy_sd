lsblk
sudo df -h
sudo umount /dev/mmcblk0
sudo dd if=/dev/mmcblk0 of=~/sd-card-copy.img bs=1M status=progress

 sudo dd if=~/sd-card-copy.img of=/dev/mmcblk0 bs=1M status=progress
 
